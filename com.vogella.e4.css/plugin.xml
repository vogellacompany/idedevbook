<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.e4.ui.css.swt.theme">
      <theme
            basestylesheeturi="css/blue.css"
            id="com.vogella.e4.css.theme.blue"
            label="Blue">
      </theme>
      <theme
            basestylesheeturi="css/rainbow.css"
            id="com.vogella.e4.css.theme.rainbow"
            label="Rainbow">
      </theme>
   </extension>
   <extension
         point="org.eclipse.e4.ui.css.core.propertyHandler">
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.CompositeElement">
         <property-name
               name="check-color">
         </property-name>
         <property-name
               name="center-color">
         </property-name>
      </handler>
   </extension>

</plugin>
